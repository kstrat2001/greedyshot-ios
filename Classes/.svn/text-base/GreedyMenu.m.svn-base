#import "GreedyMenu.h"
#import "MenuDefs.h"


@implementation GreedyMenu

-(id)init
{
    self = [super init];
    
    if(self != nil)
    {
        titleSprite = [CCLabelTTF labelWithString:[self titleText] fontName:MENU_TITLE_FONT fontSize:MENU_TITLE_FONT_SIZE];
        [titleSprite setPosition:MENU_TITLE_POS];
        [self addChild:titleSprite z:MENU_TITLE_Z];
        
    }
    
    return self;
}

-(NSString*)titleText
{
    return @"Implement Override";
}

@end
